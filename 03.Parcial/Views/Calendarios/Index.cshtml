@model IEnumerable<_03.Parcial.Calendario>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear uno nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Clase.Descripcion_Baile)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Clase.Descripcion_Baile)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id_Baile }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id_Baile }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id_Baile })
        </td>
    </tr>
}

</table>
